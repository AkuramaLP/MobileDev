<?xml version='1.0' encoding='utf-8'?>
<widget id="de.games_academy.pro2016ber.gpscatch_jw" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GPS Catch</name>
    <description>
        A spimple location based game.
    </description>
    <author email="jonathan.witt@games-academy.de" href="http://cordova.io">
        Jonathan Witt Dev
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
    <plugin name="cordova-plugin-geolocation" spec="^2.4.3" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-device-motion" spec="^1.2.5" />
    <plugin name="cordova-plugin-vibration" spec="^2.1.5" />
    <engine name="android" spec="^6.3.0" />
    <engine name="browser" spec="^5.0.1" />
</widget>
