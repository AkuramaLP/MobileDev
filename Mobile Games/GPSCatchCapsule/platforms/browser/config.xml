<?xml version='1.0' encoding='utf-8'?>
<widget id="de.games_academy.pro2016ber.gpscatch_jw" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GPS Catch</name>
    <description>
        A spimple location based game.
    </description>
    <author email="jonathan.witt@games-academy.de" href="http://cordova.io">
        Jonathan Witt Dev
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="ws://10.1.1.121" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
</widget>
